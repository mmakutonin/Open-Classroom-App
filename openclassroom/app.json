{
  "name": "openclassroom",
  "displayName": "openclassroom",
  "expo": {
      "name": "openclassroom",
      "slug": "open-classroom",
      "privacy": "unlisted",
      "sdkVersion": "35.0.0", //or current SDK version
      "version": "1.0.0",
      "entryPoint": "node_modules/expo/AppEntry.js",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
      //"orientation": "portrait",
      //"primaryColor": "#xxxxxx", //colorstring here
      //"icon": "./path/to/app/icon",
      // "splash": {
      //     "backgroundColor": "#xxxxxx", //Splash color 
      //     "image": "./path/to/splashscreen/image"
      // },
      "ios": {
          "bundleIdentifier": "com.BonaventureUniversity.AppNameNoSpaces",
           "supportsTablet": true
      },
      "android": {
          "permissions": [],
          "versionCode": 1, //make sure to iterate version code based on the update (ie if this is your initial release, code = 1, then 2, etc, etc)
          "package": "com.BonaventureUniversity.AppNameNoSpaces"
      },
      "packagerOpts": {
          "assetExts": [
      "ttf",
      "mp4",
      "otf",
      "xml" //if another file format is used, add it here
    ]
      }
    }
}
